   "source": [
    "def fib(n, table=[]):\n",
    "    if n<2:\n",
    "        table.append((n, (None, None)))\n",
    "    else:\n",
    "        n = fib(n - 1, table)[0] + fib(n - 2, table)[0]\n",
    "        table.append((n, (n-1, n-2)))\n",
    "    return n, table\n",
    "fib(3)"
   ]
  }
